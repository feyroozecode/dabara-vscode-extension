{
  "Dabara Program": {
    "prefix": "fara",
    "body": [
      "fara",
      "  $1",
      "ƙare"
    ],
    "description": "Structure complète d'un programme Dabara"
  },
  "Variable Declaration": {
    "prefix": "naɗa",
    "body": "naɗa ${1:nom} = ${2:valeur}",
    "description": "Déclaration de variable"
  },
  "Print Statement": {
    "prefix": "rubuta",
    "body": "rubuta ${1:expression}",
    "description": "Instruction d'affichage"
  },
  "Function Definition": {
    "prefix": "aiki",
    "body": [
      "aiki ${1:sunan_aiki}(${2:parametres}) {",
      "  $3",
      "  komawa ${4:sakamakon}",
      "}"
    ],
    "description": "Définition d'une fonction"
  },
  "If Statement": {
    "prefix": "idan",
    "body": [
      "idan (${1:sharaɗi}) {",
      "  $2",
      "}"
    ],
    "description": "Instruction conditionnelle"
  },
  "If Else Statement": {
    "prefix": "idansauran",
    "body": [
      "idan (${1:sharaɗi}) {",
      "  $2",
      "} sauran {",
      "  $3",
      "}"
    ],
    "description": "Instruction conditionnelle avec alternative"
  },
  "Loop Statement": {
    "prefix": "maimaita",
    "body": [
      "maimaita (${1:sharaɗi}) {",
      "  $2",
      "}"
    ],
    "description": "Boucle de répétition"
  },
  "Boolean True": {
    "prefix": "gaskiya",
    "body": "gaskiya",
    "description": "Valeur booléenne vraie"
  },
  "Boolean False": {
    "prefix": "karya", 
    "body": "karya",
    "description": "Valeur booléenne fausse"
  },
  "Addition": {
    "prefix": "ƙara",
    "body": "${1:lamba1} ƙara ${2:lamba2}",
    "description": "Opération d'addition"
  },
  "Subtraction": {
    "prefix": "rage",
    "body": "${1:lamba1} rage ${2:lamba2}",
    "description": "Opération de soustraction"
  },
  "String Variable": {
    "prefix": "naɗastring",
    "body": "naɗa ${1:sunan} = \"${2:kalmomi}\"",
    "description": "Déclaration d'une variable chaîne"
  },
  "Number Variable": {
    "prefix": "naɗanumber",
    "body": "naɗa ${1:sunan} = ${2:0}",
    "description": "Déclaration d'une variable numérique"
  },
  "Hello World": {
    "prefix": "sannu",
    "body": [
      "fara",
      "  rubuta \"Sannu duniya!\"",
      "ƙare"
    ],
    "description": "Programme Hello World en Dabara"
  },
  "Simple Calculator": {
    "prefix": "calculator",
    "body": [
      "fara",
      "  naɗa lamba1 = ${1:10}",
      "  naɗa lamba2 = ${2:5}",
      "  naɗa jimla = lamba1 ƙara lamba2",
      "  naɗa bambanci = lamba1 rage lamba2",
      "  naɗa sakamako_ninka = lamba1 ninka lamba2",
      "  naɗa sakamako_raba = lamba1 raba lamba2",
      "  ",
      "  rubuta \"Jimla: \" + jimla",
      "  rubuta \"Bambanci: \" + bambanci",
      "  rubuta \"Ninka: \" + sakamako_ninka",
      "  rubuta \"Raba: \" + sakamako_raba",
      "ƙare"
    ],
    "description": "Calculatrice complète avec tous les opérateurs"
  },
  "Multiplication": {
    "prefix": "ninka",
    "body": "${1:lamba1} ninka ${2:lamba2}",
    "description": "Opération de multiplication"
  },
  "Division": {
    "prefix": "raba",
    "body": "${1:lamba1} raba ${2:lamba2}",
    "description": "Opération de division"
  },
  "User Input": {
    "prefix": "karɓa",
    "body": [
      "rubuta \"${1:message}: \"",
      "naɗa ${2:variable} = karɓa"
    ],
    "description": "Entrée utilisateur avec message"
  },
  "Interactive Program": {
    "prefix": "interactive",
    "body": [
      "fara",
      "  rubuta \"${1:Sannu! Rubuta sunanka}: \"",
      "  naɗa suna = karɓa",
      "  rubuta \"Sannu \" + suna + \"!\"",
      "ƙare"
    ],
    "description": "Programme interactif avec entrée utilisateur"
  },
  "Comment Line": {
    "prefix": "#",
    "body": "# ${1:commentaire}",
    "description": "Commentaire ligne (style Python)"
  },
  "Dabadara Condition": {
    "prefix": "dabadara",
    "body": [
      "dabadara (${1:sharaɗi}) {",
      "  $2",
      "}"
    ],
    "description": "Condition dabadara alternative"
  },
  "Dabadara with Else": {
    "prefix": "dabadarasauran",
    "body": [
      "dabadara (${1:sharaɗi}) {",
      "  $2",
      "} sauran {",
      "  $3",
      "}"
    ],
    "description": "Condition dabadara avec alternative"
  },
  "List Declaration": {
    "prefix": "jerin",
    "body": "naɗa ${1:sunan_jerin} = [${2:abu1}, ${3:abu2}, ${4:abu3}]",
    "description": "Déclaration d'une liste"
  },
  "Empty List": {
    "prefix": "jeringudu",
    "body": "naɗa ${1:sunan_jerin} = []",
    "description": "Déclaration d'une liste vide"
  },
  "String List": {
    "prefix": "jerinstring",
    "body": "naɗa ${1:sunan_jerin} = [\"${2:abu1}\", \"${3:abu2}\", \"${4:abu3}\"]",
    "description": "Liste de chaînes de caractères"
  },
  "Number List": {
    "prefix": "jerinnumber",
    "body": "naɗa ${1:sunan_jerin} = [${2:1}, ${3:2}, ${4:3}]",
    "description": "Liste de nombres"
  },
  "Math Operations": {
    "prefix": "mathops",
    "body": [
      "# Opérateurs mathématiques Dabara",
      "naɗa a = ${1:10}",
      "naɗa b = ${2:3}",
      "naɗa jimla = a ƙara b      # Addition",
      "naɗa bambanci = a rage b   # Soustraction",
      "naɗa sakamako = a ninka b  # Multiplication",
      "naɗa rabo = a raba b       # Division"
    ],
    "description": "Toutes les opérations mathématiques"
  }
